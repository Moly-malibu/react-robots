{
  "name": "react-robots",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "5.9.1",
    "npm": "3.7.3"
  },
  "scripts": {
    "build":                  "webpack --config ./webpack.config.js --progress --colors",
    "db-seed-dev":            "node ./db/seed.js",
    "db-seed-prod":           "NODE_ENV=production node ./db/seed.js",
    "webserver-dev":          "node ./bin/www",
    "webserver-test":         "NODE_ENV=test node ./bin/www",
    "postinstall":            "npm run build",
    "heroku-postbuild":       "npm run db-seed-prod",
    "test-api":               "NODE_ENV=test mocha test/api/ --recursive",
    "test-layout-components": "NODE_ENV=test mocha test/components/ --recursive --compilers js:babel-register",
    "test-robot-components":  "NODE_ENV=test mocha test/components/robots/ --recursive --compilers js:babel-register"
  },
  "dependencies": {
    "babel-core": "^6.8.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "body-parser": "~1.13.2",
    "bootstrap": "^3.3.6",
    "cookie-parser": "~1.3.5",
    "css-loader": "^0.23.1",
    "debug": "~2.2.0",
    "ejs": "~2.3.3",
    "express": "~4.13.1",
    "file-loader": "^0.8.5",
    "json-loader": "^0.5.4",
    "moment-timezone": "^0.5.4",
    "mongoose": "^4.4.19",
    "morgan": "~1.6.1",
    "react": "^15.0.2",
    "react-dom": "^15.0.2",
    "react-router": "^2.4.0",
    "serve-favicon": "~2.3.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1"
  },
  "devDependencies": {
    "babel-preset-es2017": "^1.4.0",
    "babel-register": "^6.9.0",
    "expect": "^1.20.1",
    "mocha": "^2.5.3",
    "selenium-webdriver": "^2.53.2",
    "supertest": "^1.2.0",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.10.0"
  }
}
